QMessageBox {
    background-color: #3d3d3d;
}

QMessageBox QLabel {
    color: #efefef;
}

QMessageBox QPushButton {
    background-color: #5e5e5e;
    color: #efefef;
    border-width: 0px;
}

QMessageBox QPushButton:hover {
    background-color: #5b71a0;
}

QMessageBox QPushButton:pressed {
    background-color: #161616;
}